{% comment %} <section class="fat-footer">
  <header></header>
  <div class="container">
      <div class="row justify-content-center py-4">
        <div class="col-md-7 col-12">
          {% shape "menu", alias: "alias:footer", cache_id: "footer", cache_fixed_duration: "00:00:01", cache_tag: "alias:footer" %}
        </div>
        <div class="col-md-5 col-12 d-flex align-items-end">
          <nav class="d-flex align-items-end">
            <img class="m-1" src="{{ 'DI Logo White.svg' | asset_url }}" />
            <div>
              <div>digital@statcan</div>
              <div>Empowering people and ideas</div>
            </div>
          </nav>
        </div>
      </div>
    </nav>
  </div>
  <footer>
    <div class="container">
      <div class="row align-items-end justify-content-between">
        <div>
          <img class="sig-wht" alt="{{ 'Statistics Canada' | t }}" aria-label="{{ 'Statistics Canada' | t }}"  src="{{ 'sig-wht-en.svg' | asset_url }}" />
        </div>
        <div>
          <img class="canada" alt="{{ 'Government of Canada' | t }}" aria-label="{{ 'Government of Canada' | t }}" src="{{ 'wmms-wht.svg' | asset_url }}" />
        </div>
      </div> 
    </div>
  </footer>
</section> {% endcomment %}

<section id="footer-section">
  <div class="d-flex justify-content-center">
    <a href="https://digital.statcan.gc.ca" class="btn btn-lg btn-primary is-rounded">{{ "More Virtual Labs" | t }}</a>
  </div>
  <div class="text-center pb-5">
    <div class="powered-by-di" >{{ "Powered by" | t }}</div>
    <img src="{{ 'DI Logo.svg' | asset_url }}" alt="{{ 'Powered by Digital Innovation' | t }}" />
  </div>
  <div class="container pt-5">
    <div class="d-flex align-items-end justify-content-between">
      <img class="sig-wht" alt="{{ 'Statistics Canada' | t }}" aria-label="{{ 'Statistics Canada' | t }}"  src="{{ 'sig-wht-eng.svg' | asset_url }}" />
      <img class="canada" alt="{{ 'Government of Canada' | t }}" aria-label="{{ 'Government of Canada' | t }}" src="{{ 'Canada.svg' | asset_url }}" />
    </div>
  </div>  
</section>